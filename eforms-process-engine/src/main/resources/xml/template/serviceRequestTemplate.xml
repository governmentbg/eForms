<?xml version='1.0' encoding='UTF-8'?>
<ServiceRequest>
    <requestURI>
        <identifier>#{/data/businessKey}</identifier>
        <identifierType>
            <code>1006-100001</code>
            <name>Идентификатор на заявление за услуга</name>
        </identifierType>
    </requestURI>
    <requestDateTime>#{/data/issueDateCaseIdentifier}</requestDateTime>
    <publicService>
        <identifier>
            <identifier>#{/data/caseDataSource/data/serviceId ?? ""}</identifier>
            <identifierType>
                <code>1006-100008</code>
                <name>Идентификатор на публична услуга</name>
            </identifierType>
        </identifier>
        <name>#{/data/caseDataSource/data/serviceName ?? ""}</name>
        <language>
            <code>BG</code>
            <name>Български</name>
        </language>
        <status>
            <code>1006-050001</code>
            <name>Активна</name>
        </status>
    </publicService>
    <serviceProvider>
        <legalIdentifier>
            <identifier>#{/data/caseDataSource/data/supplier ?? ""}</identifier>
            <identifierType>
                <code>1006-100003</code>
                <name>БУЛСТАТ</name>
            </identifierType>
        </legalIdentifier>
        <identifier>
            <identifier>#{/data/caseDataSource/data/serviceSupplierId ?? ""}</identifier>
            <identifierType>
                <code>1006-100007</code>
                <name>Идентификатор на публична организация</name>
            </identifierType>
        </identifier>
        <preferredLabel>#{/data/caseDataSource/data/serviceSupplierName ?? ""}</preferredLabel>
        <organizationType>
            <code>#{/data/caseDataSource/data/serviceSupplierTypeCode ?? ""}</code>
            <name>#{/data/caseDataSource/data/serviceSupplierTypeName ?? ""}</name>
        </organizationType>
        <administrativeUnits>
            <legalIdentifier>
                <identifier>#{/data/caseDataSource/data/administrationUnitEDelivery ?? ""}</identifier>
                <identifierType>
                    <code>1006-100003</code>
                    <name>БУЛСТАТ</name>
                </identifierType>
            </legalIdentifier>
            <identifier>
                <identifier>#{/data/caseDataSource/data/serviceSupplierId ?? ""}</identifier>
                <identifierType>
                    <code>1006-100007</code>
                    <name>Идентификатор на публична организация</name>
                </identifierType>
            </identifier>
            <preferredLabel>#{/data/caseDataSource/data/administrationUnit ?? ""}</preferredLabel>
            <organizationType>
                <code>#{/data/caseDataSource/data/serviceSupplierTypeCode ?? ""}</code>
                <name>#{/data/caseDataSource/data/serviceSupplierTypeName ?? ""}</name>
            </organizationType>
        </administrativeUnits>
    </serviceProvider>
    <requestType>
        <code>0006-000121</code>
        <name>Първоначално заявление за предоставяне на електронна административна услуга</name>
    </requestType>
    <resultChannel>
        <identifier>#{/data/channel/channelType}</identifier>
        <channelType>
            <code>#{/data/channel/channelType}</code>
            <name>#{/data/channel/channelName}</name>
        </channelType>
    </resultChannel>
    <processTime>
        <processTimeType>
            <code>#{/data/deadline/deadlineType/deadlineTypeType ?? 0006-000083}</code>
            <name>#{/data/deadline/deadlineType/deadlineTypeName ?? Нормална услуга}</name>
        </processTimeType>
        <processTimeUnit>
            <code>#{/data/deadline/deadlineUnit/deadlineUnitType ?? workDays}</code>
            <name>#{/data/deadline/deadlineUnit/deadlineUnitName ?? Работни дни}</name>
        </processTimeUnit>
        <processTime>#{/data/deadline/deadlineValue ?? 0}</processTime>
    </processTime>
    <tax>
        <amount>#{/data/tax/taxAmount ?? 0}</amount>
        <currency>
            <code>#{/data/tax/taxCurrency}</code>
            <name>#{/data/tax/taxCurrency}</name>
        </currency>
        <validFrom type="date">#{/data/tax/validFrom}</validFrom>
        <validТо type="date">#{/data/tax/validTo}</validТо>
    </tax>
    <communicationChannel>
        <contactType>
            <code>1006-030006</code>
            <name>Електронна поща</name>
        </contactType>
        <contact>#{/data/applicantEmailAddress}</contact>
    </communicationChannel>
    <communicationChannel>
        <contactType>
            <code>1006-030005</code>
            <name>Телефон</name>
        </contactType>
        <contact>#{/data/applicantPhoneNumber}</contact>
    </communicationChannel>
