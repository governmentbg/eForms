            <identifier>
                <identifier>#{/data/applicantPhysicalIdentifier}</identifier>
                <identifierType>
                    <code>#{/data/applicantPhysicalIdentifierType}</code>
                    <name>#{/data/applicantPhysicalIdentifierType}</name>
                </identifierType>
            </identifier>
            <fullName>#{/data/applicantPhysicalFirstName} #{/data/applicantPhysicalMiddleName} #{/data/applicantPhysicalFamilyName}</fullName>
            <firstName>#{/data/applicantPhysicalFirstName}</firstName>
            <middleName>#{/data/applicantPhysicalMiddleName}</middleName>
            <familyName>#{/data/applicantPhysicalFamilyName}</familyName>
            <gender>
                <code>#{/data/applicantGenderCode}</code>
                <name>#{/data/applicantGender}</name>
            </gender>
            <fullNameLatin>#{/data/applicantPhysicalFirstNameLatin} #{/data/applicantPhysicalMiddleNameLatin} #{/data/applicantPhysicalFamilyNameLatin}</fullNameLatin>
            <firstNameLatin>#{/data/applicantPhysicalFirstNameLatin}</firstNameLatin>
            <middleNameLatin>#{/data/applicantPhysicalMiddleNameLatin}</middleNameLatin>
            <familyNameLatin>#{/data/applicantPhysicalFamilyNameLatin}</familyNameLatin>
            <identificationDocument>
                <documentType>
                    <code>#{/data/applicantPhysicalDocumentTypeCode}</code>
                    <name>#{/data/applicantPhysicalDocumentType}</name>
                </documentType>
                <documentNumber>#{/data/applicantPhysicalDocumentNumber}</documentNumber>
                <issueDate>#{/data/applicantPhysicalDocumentIssueDate}</issueDate>
                <issuer>#{/data/applicantPhysicalDocumentIssuer}</issuer>
                <regix>
                    <synchronized>#{/data/applicantIdentificationDocumentRegix}</synchronized>
                    <dateTime>#{/data/applicantIdentificationDocumentRegixDateTime}</dateTime>
                </regix>
            </identificationDocument>
            <permanentAddress>
                <country>
                    <code>#{/data/applicantPhysicalPermAddrCountryCode}</code>
                    <name>#{/data/applicantPhysicalPermAddrCountry}</name>
                </country>
                <address>
                    <district>
                        <code>#{/data/applicantPhysicalPermAddrDistrictSelect/districtCode}</code>
                        <name>#{/data/applicantPhysicalPermAddrDistrictSelect/districtName}</name>
                    </district>
                    <municipality>
                        <code>#{/data/applicantPhysicalPermAddrMunicipalitySelect/municipalityCode}</code>
                        <name>#{/data/applicantPhysicalPermAddrMunicipalitySelect/municipalityName}</name>
                    </municipality>
                    <settlement>
                        <code>#{/data/applicantPhysicalPermAddrSettlementSelect/settlementCode}</code>
                        <name>#{/data/applicantPhysicalPermAddrSettlementSelect/settlementName}</name>
                    </settlement>
                    <locationName>#{/data/applicantPhysicalPermAddrLocationName}</locationName>
                    <buildingNumber>#{/data/applicantPhysicalPermAddrBuildingNumber}</buildingNumber>
                    <entrance>#{/data/applicantPhysicalPermAddrEntrance}</entrance>
                    <floor>#{/data/applicantPhysicalPermAddrFloor}</floor>
                    <apartment>#{/data/applicantPhysicalPermAddrApartment}</apartment>
                    <fullAddress>#{/data/applicantPhysicalPermAddrFullAddress}</fullAddress>
                </address>
                <regix>
                    <synchronized>#{/data/applicantPermAddrRegix}</synchronized>
                    <dateTime>#{/data/applicantPermAddrRegixDateTime}</dateTime>
                </regix>
            </permanentAddress>
            <presentAddress>
                <country>
                    <code>#{/data/applicantPhysicalPresAddrCountryCode}</code>
                    <name>#{/data/applicantPhysicalPresAddrCountry}</name>
                </country>
                <address>
                    <district>
                        <code>#{/data/applicantPhysicalPresAddrDistrictSelect/districtCode}</code>
                        <name>#{/data/applicantPhysicalPresAddrDistrictSelect/districtName}</name>
                    </district>
                    <municipality>
                        <code>#{/data/applicantPhysicalPresAddrMunicipalitySelect/municipalityCode}</code>
                        <name>#{/data/applicantPhysicalPresAddrMunicipalitySelect/municipalityName}</name>
                    </municipality>
                    <settlement>
                        <code>#{/data/applicantPhysicalPresAddrSettlementSelect/settlementCode}</code>
                        <name>#{/data/applicantPhysicalPresAddrSettlementSelect/settlementName}</name>
                    </settlement>
                    <locationName>#{/data/applicantPhysicalPresAddrLocationName}</locationName>
                    <buildingNumber>#{/data/applicantPhysicalPresAddrBuildingNumber}</buildingNumber>
                    <entrance>#{/data/applicantPhysicalPresAddrEntrance}</entrance>
                    <floor>#{/data/applicantPhysicalPresAddrFloor}</floor>
                    <apartment>#{/data/applicantPhysicalPresAddrApartment}</apartment>
                    <fullAddress>#{/data/applicantPhysicalPresAddrFullAddress}</fullAddress>
                </address>
                <regix>
                    <synchronized>#{/data/applicantPresAddrRegix}</synchronized>
                    <dateTime>#{/data/applicantPresAddrRegixDateTime}</dateTime>
                </regix>
            </presentAddress>
            <regix>
                <synchronized>#{/data/applicantMainDataRegix}</synchronized>
                <dateTime>#{/data/applicantMainDataRegixDateTime}</dateTime>
            </regix>
        </person>
    </applicant>
    <requestAuthor>
        <person>
            <identifier>
                <identifier>#{/data/requestorIdentifier}</identifier>
                <identifierType>
                    <code>#{/data/requestorIdentifierType}</code>
                    <name>#{/data/requestorIdentifierType}</name>
                </identifierType>
            </identifier>
            <fullName>#{/data/requestorFirstName} #{/data/requestorMiddleName} #{/data/requestorFamilyName}</fullName>
            <firstName>#{/data/requestorFirstName}</firstName>
            <middleName>#{/data/requestorMiddleName}</middleName>
            <familyName>#{/data/requestorFamilyName}</familyName>
            <gender>
                <code>#{/data/requestorGenderCode}</code>
                <name>#{/data/requestorGender}</name>
            </gender>
            <fullNameLatin>#{/data/requestorPhysicalFirstNameLatin} #{/data/requestorPhysicalMiddleNameLatin} #{/data/requestorPhysicalFamilyNameLatin}</fullNameLatin>
            <firstNameLatin>#{/data/requestorPhysicalFirstNameLatin}</firstNameLatin>
            <middleNameLatin>#{/data/requestorPhysicalMiddleNameLatin}</middleNameLatin>
            <familyNameLatin>#{/data/requestorPhysicalFamilyNameLatin}</familyNameLatin>
            <regix>
                <synchronized>#{/data/requestorMainDataRegix}</synchronized>
                <dateTime>#{/data/requestorMainDataRegixDateTime}</dateTime>
            </regix>
        </person>
        <authorType>
            <code>#{/data/requestorTypeSelect/authorTypeCode ?? 1006-020001}</code>
            <name>#{/data/requestorTypeSelect/authorType ?? Титуляр}</name>
        </authorType>
    </requestAuthor>
    <dataProcessingAgreement>#{/data/dataProcessingAgreement ?? true}</dataProcessingAgreement>
    <specificContent>#{/data/specificContent/*}</specificContent>
